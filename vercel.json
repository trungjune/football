{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "backend/dist/**",
          "backend/prisma/**",
          "backend/node_modules/.prisma/**",
          "backend/node_modules/@prisma/**",
          "backend/src/**"
        ],
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/api/index.ts?path=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
